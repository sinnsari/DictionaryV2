@model EngDictionary
@{
    ViewData["Title"] = "New";
}

<form asp-controller="Dictionary" asp-action="New" method="post">

    <div class="form-group">
        <label asp-for="EngStr">English : </label>
        <input asp-for="EngStr" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="TrStr">Turkish : </label>
        <input asp-for="TrStr" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">New One!</button>
</form>

